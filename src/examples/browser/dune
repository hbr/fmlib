(executable
  (name webapp)
  (modules webapp)
  (modes js)
  (libraries fmlib_browser)
)

(rule
    (targets webapp.js)
    (deps    webapp.bc.js)
    (mode   (promote (until-clean)))
    (action (copy %{deps} %{targets}))
)




